{
    "manifest_version": 3,
    "name": "Panopto Speed Control",
    "version": "1.0",
    "description": "Adjust playback speed of Panopto videos.",
    "permissions": [
        "activeTab",
        "scripting",
        "storage"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icon.png",
            "48": "icon.png",
            "128": "icon.png"
        }
    }
}